<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |us| %>
      <tr>
        <td><%= us.email %></td>
        <td><%= link_to 'Show', admins_user_path(us) %></td>
        <td><%= link_to 'Edit', edit_admins_user_path(us) %></td>
        <td><%= link_to 'Destroy', admins_user_path(us), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_admins_user_path %>
